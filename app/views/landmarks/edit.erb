<h2>Edit Historical Landmark</h2>


<form method="post" action="/landmarks/<%= @landmark.id %>">
<input id ="hidden" type="hidden" name="_method" value="patch">
<p>Name: <input type="text" name="landmark[name]" id="name" value="<%= @landmark.name %>"></p>
<p>Year Created: <input type="text" name="landmark[year_completed]" id="year_completed" value="<%= @landmark.year_completed %>"></p>

<input type="submit" id="edit" value="Edit Landmark">
</form>

<a class="button" href="/figures">Landmarks</a>
<a class="button" href="/">Home</a>
